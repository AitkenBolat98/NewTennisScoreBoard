<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>NewMatch</servlet-name>
    <servlet-class>servlets.NewMatch</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewMatch</servlet-name>
    <url-pattern>/new-match</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MatchScore</servlet-name>
    <servlet-class>servlets.MatchScore</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MatchScore</servlet-name>
    <url-pattern>/match-score</url-pattern>
  </servlet-mapping>
</web-app>
